\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">Username</div>\r\n            <v-text-field\r\n              v-model=\"username\"\r\n              :rules=\"[rules.required]\"\r\n              solo\r\n              class=\"mt-1 mx-2\"\r\n              dense\r\n              background-color=\"white\"\r\n              light\r\n              width=\"10px\"\r\n              label=\" enter your username\"  \r\n            /> \r\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">Display name</div>\r\n            <v-text-field\r\n              v-model=\"displayName\"\r\n              :rules=\"[rules.required]\"\r\n              solo\r\n              class=\"mt-1 mx-2\"\r\n              dense\r\n              background-color=\"white\"\r\n              light\r\n              width=\"10px\"\r\n              label=\" enter your display name\"        \r\n            /> \r\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">Password</div>\r\n            <v-text-field\r\n              v-model=\"password\"\r\n              :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\r\n              :type=\"show ? 'text' : 'password'\"\r\n              :rules=\"[rules.required, rules.min, rules.max]\"\r\n              @click:append=\"show=!show\"\r\n              hint=\"At least 8 characters\"\r\n              solo\r\n              class=\"mt-1 mx-2\"\r\n              dense\r\n              background-color=\"white\"\r\n              light\r\n              width=\"10px\"\r\n              label=\" enter your password\"\r\n            /> \r\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">Confirm password</div>\r\n            <v-text-field\r\n              v-model=\"confirmPassword\"\r\n              :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\r\n              :type=\"show ? 'text' : 'password'\"\r\n              :rules=\"[rules.required]\"\r\n              @click:append=\"show=!show\"\r\n              :error-messages='passwordMatch()'\r\n              hint=\"Must be the same as password above\"\r\n              solo\r\n              class=\"mt-1 mx-2\"\r\n              dense\r\n              background-color=\"white\"\r\n              light\r\n              width=\"10px\"\r\n              label=\"re-enter your password\"        \r\n            /> \r\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">E-mail address</div>\r\n            <v-text-field\r\n              v-model=\"email\"\r\n              :rules=\"[rules.required,rules.emailRule]\"\r\n              solo\r\n              class=\"mt-1 mx-2\"\r\n              dense\r\n              background-color=\"white\"\r\n              light\r\n              width=\"10px\"\r\n              label=\" enter your e-mail address\"        \r\n            /> \r\n            <div class=\"mt-6 mx-3 subtitle-1 font-weight-medium\">Favourite Game tag</div>\r\n              <div class=\"mt-1 ml-1\">\r\n                <v-dialog\r\n                  v-model=\"chooseTag\"\r\n                  width=\"350\"\r\n                >\r\n                  <template v-slot:activator=\"{ on }\">\r\n                    <v-btn\r\n                      color=\"grey darken-1\"\r\n                      outlined\r\n                      small\r\n                      class=\"mx-2 mb-2\"\r\n                      v-on=\"on\"\r\n                      @click=\"getTag();\"\r\n                    >\r\n                      Choose here\r\n                    </v-btn>\r\n                  </template>\r\n                  <v-card light>\r\n                    <v-card-title\r\n                      class=\"headline grey darken-4 white--text\"\r\n                      primary-title\r\n                    >\r\n                    Add favourite tag(s)\r\n                    </v-card-title>\r\n                    <v-container>\r\n                      <div class=\"font-weight-medium headline text-left ml-1 my-2\">Tag list</div>\r\n                      <v-divider></v-divider>\r\n                      <div v-for=\"item in tagList\" v-bind:key=\"item._id\">\r\n                          <v-checkbox class=\"ml-1 my-3\"\r\n                            v-model=\"favTag\" \r\n                            :label=item.name \r\n                            :value=item._id \r\n                            :rules=\"[rules.tagRule]\"\r\n                            hide-details\r\n                          >\r\n                          </v-checkbox>\r\n                      </div>\r\n                      <v-divider></v-divider>\r\n                      <v-btn\r\n                        color=\"grey darken-1\"\r\n                        outlined\r\n                        small\r\n                        class=\"ml-1 mt-4 mb-2\"\r\n                        @click=\"chooseTag=false\"\r\n                      >\r\n                      Comfirm\r\n                      </v-btn>\r\n                      <v-btn\r\n                        color=\"grey darken-1\"\r\n                        outlined\r\n                        small\r\n                        class=\"mx-5 mt-4 mb-2\"\r\n                        @click=\"chooseTag=false; favTag=[];\"\r\n                      >\r\n                      Cancel\r\n                      </v-btn>\r\n                    </v-container>\r\n                  </v-card>\r\n                </v-dialog>\r\n              </div>\r\n            </v-form>\r\n          </v-container>\r\n          <v-divider class=\"mt-3\"></v-divider>\r\n          <v-card-actions>\r\n            <v-btn\r\n              :disabled=\"!valid\"\r\n              color=\"grey darken-1\"\r\n              outlined\r\n              class=\"mx-3 mt-4 mb-2\"\r\n              @click=\"submitUser\"\r\n            >\r\n              Sign up\r\n            </v-btn>\r\n            <v-btn\r\n              color=\"grey darken-1\"\r\n              outlined\r\n              class=\"mx-2 mt-4 mb-2\"\r\n              @click=\"register = false\"\r\n            >\r\n              Cancel\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>           \r\n  </v-app-bar>\r\n</nav>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Bar\",\r\n  components: {\r\n  },\r\n  data: () => ({\r\n    login: false,\r\n    register: false,\r\n    show: false,\r\n    chooseTag:false,\r\n    valid:true,\r\n    tagList: [],\r\n\r\n    avatar: \"/Tantrum/src/image/pepe_meme.jpg\",\r\n    username: \"\",\r\n    displayName: \"\",\r\n    password: \"\",\r\n    confirmPassword: \"\",\r\n    email: \"\",\r\n    favTag: [],\r\n    rules: {\r\n      required: value => !!value || 'Required',\r\n      min: v => v.length >= 8 || 'Min 8 characters',\r\n      max: v => v.length <= 16 || 'Max 16 characters',\r\n      emailRule: v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\r\n      tagRule: v=>v.length > 0 || 'Please select at least 1 tag'\r\n    }\r\n  }),\r\n  methods: {\r\n    passwordMatch () { \r\n    return (this.password === this.confirmPassword) ? '' : 'Password must match'\r\n    },\r\n    getTag(){\r\n      this.$http.get(\"/tag\").then(response => (this.tagList = response.data))\r\n    }\r\n    ,\r\n    submitUser(){\r\n        this.$http(\"/user\",\r\n          {\r\n            avatar: this.avatar,\r\n            username: this.username,\r\n            displayName: this.displayName,\r\n            password: this.password,\r\n            email: this.email,\r\n            favTag: this.favTag\r\n          }).then(response => (response))\r\n    },\r\n    validate () {\r\n      if (this.$refs.form.validate()) {\r\n        this.snackbar = true\r\n        return 1\r\n    }else{\r\n      return 0\r\n    }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .link { \r\n    text-decoration: none;\r\n    color: #FFF\r\n  }\r\n</style>"]}]}